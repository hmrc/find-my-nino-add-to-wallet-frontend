@import models.individualDetails.IndividualDetailsDataCache

        @this()

        @(individualDetailsDataCache: IndividualDetailsDataCache, date: String, lang: String)(implicit messages: Messages)


        <!-- example for a simple fo file. At the beginning the page layout is set.
          Below fo:root there is always
        - a single fo:layout-master-set which defines one or more page layouts
        - an optional fo:declarations
        - and a sequence of one or more fo:page-sequences containing the text and formatting instructions
        -->

<fo:root
        xmlns:fo="http://www.w3.org/1999/XSL/Format"
        xmlns:fox="http://xmlgraphics.apache.org/fop/extensions"
        font-family="OpenSans"
        xml:lang="@lang"
>

    <fo:layout-master-set>
        <!-- fo:layout-master-set defines in its children the page layout:
             the pagination and layout specifications
            - page-masters: have the role of describing the intended subdivisions
                             of a page and the geometry of these subdivisions
                            In this case there is only a simple-page-master which defines the
                            layout for all pages of the text
        -->

        <!-- layout for the first page -->
        <fo:simple-page-master master-name="first"
                               page-height="29.7cm"
                               page-width="21cm"
                               margin-top="0.5cm"
                               margin-bottom="0.5cm"
                               margin-left="2.2cm"
                               margin-right="2.2cm">
            <fo:region-body margin-top="0.7cm"/>
            <fo:region-before extent="0.5cm"/>
            <fo:region-after extent="0.6cm"/>
        </fo:simple-page-master>

        <!-- layout for the other pages -->
        <fo:simple-page-master master-name="rest"
                               page-height="29.7cm"
                               page-width="21cm"
                               margin-top="0.5cm"
                               margin-bottom="0.5cm"
                               margin-left="2.2cm"
                               margin-right="2.2cm">
            <fo:region-body margin-top="0.5cm"/>
            <fo:region-before extent="0.5cm"/>
            <fo:region-after extent="0.6cm"/>
        </fo:simple-page-master>

        <fo:page-sequence-master master-name="basicPSM">
            <fo:repeatable-page-master-alternatives>
                <fo:conditional-page-master-reference master-reference="first"
                                                      page-position="first"/>
                <fo:conditional-page-master-reference master-reference="rest"
                                                      page-position="rest"/>
                <!-- recommended fallback procedure -->
                <fo:conditional-page-master-reference master-reference="rest"/>
            </fo:repeatable-page-master-alternatives>
        </fo:page-sequence-master>

    </fo:layout-master-set>
    <!-- end: defines page layout -->

    <!-- Document metadata -->
    <fo:declarations>
        <x:xmpmeta xmlns:x="adobe:ns:meta/">
            <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
                <rdf:Description xmlns:dc="http://purl.org/dc/elements/1.1/">
                    <dc:title>
                        @Messages("label.your_national_insurance_number_letter")
                    </dc:title>
                </rdf:Description>
            </rdf:RDF>
        </x:xmpmeta>
    </fo:declarations>
    <!-- end Document metadata -->

    <fo:bookmark-tree>
        <fo:bookmark internal-destination="heading-h1-1">
            <fo:bookmark-title>@Messages("label.your_national_insurance_number")</fo:bookmark-title>
            <fo:bookmark internal-destination="heading-h1-1-h2-1">
                <fo:bookmark-title>@messages("label.your_national_insurance_number_is")</fo:bookmark-title>
            </fo:bookmark>
        </fo:bookmark>
    </fo:bookmark-tree>

    <!-- start page-sequence
         here comes the text (contained in flow objects)
         the page-sequence can contain different fo:flows
         the attribute value of master-name refers to the page layout
         which is to be used to layout the text contained in this
         page-sequence-->
    <fo:page-sequence master-reference="basicPSM">

        <!-- footer -->
        <fo:static-content role="artifact" flow-name="xsl-region-after">
            <fo:block font-size="9pt" font-family="OpenSans" font-weight="normal" line-height="14pt">
                <!-- date -->
                <fo:block text-align="end">
                    @Messages("label.hmrc_date", date)
                </fo:block>
            </fo:block>
        </fo:static-content>
        <!-- end footer -->
        <fo:flow flow-name="xsl-region-body">

            <!-- logo and heading -->
            <fo:block role="Div" space-after="-3px">
                <fo:inline-container role="Div" inline-progression-dimension="23%">
                    <fo:block role="Div"
                              border-left-style="solid"
                              border-width="2px"
                              border-color="#28a197"
                              padding-start="4px">
                        <fo:wrapper role="artifact">
                            <fo:external-graphic content-type="content-type:image/png"
                                                 src="/pdf/logo/hmrc-logo.jpg"
                                                 content-height="scale-to-fit"
                                                 content-width="0.8cm"
                                                 role="Artifact"
                                                 fox:alt-text="HMRC logo"
                            />
                        </fo:wrapper>
                        <fo:block role="P"
                                  font-family="OpenSans"
                                  font-weight="normal"
                                  line-height="14pt"
                                  font-size="14pt"
                                  padding-before="-14px"
                                  linefeed-treatment="preserve">
                            @Messages("label.hm_revenue_customs_ni_print.revenue.pdf")
                            @Messages("label.hm_revenue_customs_ni_print.customs.pdf")
                        </fo:block>
                    </fo:block>
                </fo:inline-container>
                <fo:inline-container role="Div" inline-progression-dimension="76%">
                    <fo:block role="H1"
                              text-align="end"
                              padding-before="10px"
                              padding-after="6px"
                              font-family="OpenSans-Bold"
                              font-weight="bold"
                              id="heading-h1-1">
                        @Messages("label.your_national_insurance_number")
                    </fo:block>
                </fo:inline-container>
            </fo:block>
            <!-- end logo and heading -->

            <!-- Addresses -->
            <fo:block role="Div">
                <fo:inline-container role="Div" inline-progression-dimension="62%">
                    <fo:block>
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt">
                            <fo:block>@individualDetailsDataCache.individualDetailsData.initialsName</fo:block>
                        </fo:block>
                        @individualDetailsDataCache.getAddressLines.map { line =>
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt">
                            <fo:block>@line</fo:block>
                        </fo:block>
                        }
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt">
                            <fo:block>@individualDetailsDataCache.getPostCode.getOrElse("").toUpperCase</fo:block>
                        </fo:block>
                    </fo:block>
                </fo:inline-container>
                <fo:inline-container role="Div" inline-progression-dimension="37%">
                    <fo:block>
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt">
                            <fo:block>
                                @Messages("label.nic_eo_hmrc_address.line1.pdf")
                            </fo:block>
                        </fo:block>
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt">
                            <fo:block>
                                @Messages("label.nic_eo_hmrc_address.line2")
                            </fo:block>
                        </fo:block>
                        @if(lang.eq("cy")) {
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt">
                            <fo:block>
                                @Messages("label.nic_eo_hmrc_address.line3")
                            </fo:block>
                        </fo:block>
                        }
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt">
                            <fo:block>
                                @Messages("label.nic_eo_hmrc_address.postcode")
                            </fo:block>
                        </fo:block>
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt" padding-after="6px" padding-before="6px">
                            <fo:block>
                                @Messages("label.phone_text")
                                @Messages("label.contact_number")
                            </fo:block>
                        </fo:block>
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt">
                            <fo:basic-link color="#1F70B8"
                                           external-destination='https://@Messages("label.www_gov_uk_hmrc")'
                                           text-decoration="underline"
                                           role="Link"
                                           fox:alt-text="Gov.uk website">
                                @messages("label.www_gov_uk_hmrc")
                            </fo:basic-link>
                        </fo:block>
                    </fo:block>
                </fo:inline-container>
            </fo:block>
            <!-- end Addresses -->

            <!-- name and title -->
            <fo:block font-size="10.5pt" font-family="OpenSans" font-weight="normal" line-height="12pt"
                      padding-after="6px" padding-before="6px">
                @individualDetailsDataCache.individualDetailsData.fullName
            </fo:block>
            <!-- end name and title -->

            <!-- NINO number box -->
            <fo:block role="Div"
                      space-after="10px"
                      background-color="#DAF4F2"
                      border-style="solid"
                      border-width="1.5px"
                      border-color="#28a197">
                <fo:block role="H2"
                          font-size="10.5pt"
                          font-family="OpenSans-Bold"
                          font-weight="bold"
                          padding-before="8px"
                          padding-after="6px"
                          text-align="center"
                          id="heading-h1-1-h2-1">
                    @messages("label.your_national_insurance_number_is")
                </fo:block>
                <fo:block role="P"
                          font-size="20pt"
                          text-align="center"
                          font-family="OpenSans-Bold"
                          font-weight="bold">
                    @individualDetailsDataCache.individualDetailsData.nino.grouped(2).mkString(" ")
                </fo:block>
            </fo:block>
            <fo:block role="P"
                      space-after="10px"
                      text-align="center"
                      font-family="OpenSans-Bold"
                      font-weight="bold"
                      padding-before="6px"
                      padding-after="6px">
                <fo:block>
                    @Messages("label.keep_this_number_in_a_safe_place_do_not_destroy_this_letter")
                </fo:block>
            </fo:block>
            <!-- end NINO number box -->

            <!-- about NINO information -->
            <fo:block role="Div">
                <fo:inline-container role="Div" inline-progression-dimension="62%">
                    <fo:block role="Div"
                              margin-left="5px"
                              margin-right="5px">
                        <fo:block role="H3" font-size="10.5pt" font-family="OpenSans-Bold" font-weight="bold"
                                  padding-before="2px" padding-after="5px">
                            <fo:block>
                                @Messages("label.about_your_national_insurance_number")
                            </fo:block>
                        </fo:block>
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt" padding-after="6px" padding-before="6px">
                            <fo:block>
                                @Messages("label.your_national_insurance_number_is_unique_to_you_and_will_never_change_")
                            </fo:block>
                        </fo:block>
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt" padding-after="6px" padding-before="6px">
                            <fo:block>
                                @Messages("label.you_will_need_it_if_you")
                            </fo:block>
                        </fo:block>
                        <fo:list-block role="L" provisional-distance-between-starts="0.3cm">
                            <fo:list-item font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                          line-height="12pt" padding-after="3px" padding-before="3px" role="LI">
                                <fo:list-item-label role="Lbl" end-indent="1em">
                                    <fo:block>
                                        <fo:inline>
                                            <fo:wrapper role="artifact">
                                                &#8226;
                                            </fo:wrapper>
                                        </fo:inline>
                                    </fo:block>
                                </fo:list-item-label>
                                <fo:list-item-body role="LBody" start-indent="2em">
                                    <fo:block>
                                        <fo:block>
                                            @Messages("label.start_work_including_part_time_and_weekend_jobs")
                                        </fo:block>
                                    </fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                            <fo:list-item font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                          line-height="12pt" padding-after="3px" padding-before="3px" role="LI">
                                <fo:list-item-label role="Lbl" end-indent="1em">
                                    <fo:block>
                                        <fo:inline>
                                            <fo:wrapper role="artifact">
                                                &#8226;
                                            </fo:wrapper>
                                        </fo:inline>
                                    </fo:block>
                                </fo:list-item-label>
                                <fo:list-item-body role="LBody" start-indent="2em">
                                    <fo:block>
                                        <fo:block>
                                            @Messages("label.apply_for_a_driving_licence")
                                        </fo:block>
                                    </fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                            <fo:list-item font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                          line-height="12pt" padding-after="3px" padding-before="3px" role="LI">
                                <fo:list-item-label role="Lbl" end-indent="1em">
                                    <fo:block>
                                        <fo:inline>
                                            <fo:wrapper role="artifact">
                                                &#8226;
                                            </fo:wrapper>
                                        </fo:inline>
                                    </fo:block>
                                </fo:list-item-label>
                                <fo:list-item-body role="LBody" start-indent="2em">
                                    <fo:block>
                                        <fo:block>
                                            @Messages("label.apply_for_a_student_loan")
                                        </fo:block>
                                    </fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                            <fo:list-item font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                          line-height="12pt" padding-after="3px" padding-before="3px" role="LI">
                                <fo:list-item-label role="Lbl" end-indent="1em">
                                    <fo:block>
                                        <fo:inline>
                                            <fo:wrapper role="artifact">
                                                &#8226;
                                            </fo:wrapper>
                                        </fo:inline>
                                    </fo:block>
                                </fo:list-item-label>
                                <fo:list-item-body role="LBody" start-indent="2em">
                                    <fo:block>
                                        <fo:block>
                                            @Messages("label.claim_state_benefits")
                                        </fo:block>
                                    </fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                            <fo:list-item font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                          line-height="12pt" padding-after="3px" padding-before="3px" role="LI">
                                <fo:list-item-label role="Lbl" end-indent="1em">
                                    <fo:block>
                                        <fo:inline>
                                            <fo:wrapper role="artifact">
                                                &#8226;
                                            </fo:wrapper>
                                        </fo:inline>
                                    </fo:block>
                                </fo:list-item-label>
                                <fo:list-item-body role="LBody" start-indent="2em">
                                    <fo:block>
                                        <fo:block>
                                            @Messages("label.register_to_vote")
                                        </fo:block>
                                    </fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                        </fo:list-block>
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt" padding-after="6px" padding-before="6px">
                            <fo:block>
                                @Messages("label.it_is_not_proof_of")
                            </fo:block>
                        </fo:block>
                        <fo:list-block role="L" provisional-distance-between-starts="0.3cm">
                            <fo:list-item font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                          line-height="12pt" padding-after="3px" padding-before="3px" role="LI">
                                <fo:list-item-label role="Lbl" end-indent="1em">
                                    <fo:block>
                                        <fo:inline>
                                            <fo:wrapper role="artifact">
                                                &#8226;
                                            </fo:wrapper>
                                        </fo:inline>
                                    </fo:block>
                                </fo:list-item-label>
                                <fo:list-item-body role="LBody" start-indent="2em">
                                    <fo:block>
                                        <fo:block>
                                            @Messages("label.your_identity")
                                        </fo:block>
                                    </fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                            <fo:list-item font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                          line-height="12pt" padding-after="3px" padding-before="3px" role="LI">
                                <fo:list-item-label role="Lbl" end-indent="1em">
                                    <fo:block>
                                        <fo:inline>
                                            <fo:wrapper role="artifact">
                                                &#8226;
                                            </fo:wrapper>
                                        </fo:inline>
                                    </fo:block>
                                </fo:list-item-label>
                                <fo:list-item-body role="LBody" start-indent="2em">
                                    <fo:block>
                                        <fo:block>
                                            @Messages("label.your_right_to_work_in_the_uk")
                                        </fo:block>
                                    </fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                        </fo:list-block>
                        <fo:block role="H3" font-size="10.5pt" font-family="OpenSans-Bold" font-weight="bold"
                                  padding-before="8px" padding-after="6px">
                            <fo:block>
                                @Messages("label.child.trust.fund")
                            </fo:block>
                        </fo:block>
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt" padding-after="6px" padding-before="6px">
                            <fo:block>
                                @Messages("label.child.trust.fund.details")
                            </fo:block>
                            <fo:basic-link color="#1F70B8"
                                           external-destination='https://@Messages("label.child.trust.fund.details.link")'
                                           text-decoration="underline"
                                           role="Link"
                                           fox:alt-text="Child Trust Fund details link">
                                @messages("label.child.trust.fund.details.link")
                            </fo:basic-link>
                        </fo:block>

                        <!-- only display welsh language section when in English -->
                        @if(lang.eq("en")) {
                        <fo:block role="H3" font-size="10.5pt" font-family="OpenSans-Bold" font-weight="bold"
                                  padding-before="8px" padding-after="6px">
                            <fo:block>
                                @Messages("label.welsh_language")
                            </fo:block>
                        </fo:block>
                        <fo:block role="P" font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                  line-height="12pt" padding-after="6px" padding-before="6px">
                            <fo:block>
                                @Messages("label.to_continue_to_receive_a_welsh_language_service_")
                            </fo:block>
                        </fo:block>
                        <fo:list-block role="L" provisional-distance-between-starts="0.3cm">
                            <fo:list-item font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                          line-height="12pt" padding-after="3px" padding-before="3px" role="LI">
                                <fo:list-item-label role="Lbl" end-indent="1em">
                                    <fo:block>
                                        <fo:inline>
                                            <fo:wrapper role="artifact">
                                                &#8226;
                                            </fo:wrapper>
                                        </fo:inline>
                                    </fo:block>
                                </fo:list-item-label>
                                <fo:list-item-body role="LBody" start-indent="2em">
                                    <fo:block>
                                        @Messages("label.email")
                                        <fo:basic-link color="#1F70B8"
                                                       external-destination='mailto:@Messages("label.email_for_welsh_language")'
                                                       text-decoration="underline"
                                                       role="Link"
                                                       fox:alt-text="Email for Welsh language">
                                            @messages("label.email_for_welsh_language")
                                        </fo:basic-link>
                                    </fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                            <fo:list-item font-size="10.5pt" font-family="OpenSans" font-weight="normal"
                                          line-height="12pt" padding-after="3px" padding-before="3px" role="LI">
                                <fo:list-item-label role="Lbl" end-indent="1em">
                                    <fo:block>
                                        <fo:inline>
                                            <fo:wrapper role="artifact">
                                                &#8226;
                                            </fo:wrapper>
                                        </fo:inline>
                                    </fo:block>
                                </fo:list-item-label>
                                <fo:list-item-body role="LBody" start-indent="2em">
                                    <fo:block>
                                        <fo:block>
                                            @Messages("label.phone.number.for.welsh.language")
                                        </fo:block>
                                    </fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                        </fo:list-block>
                        }
                    </fo:block>
                </fo:inline-container>
                <fo:inline-container role="Div" inline-progression-dimension="37%">
                    <fo:block role="Div"
                              background-color="#DAF4F2"
                              margin-left="5px"
                              margin-right="5px"
                              padding-start="5px"
                              padding-end="5px"
                              padding-before="2px"
                              padding-after="5px">
                        <fo:block role="H3"
                                  border-after-style="solid"
                                  border-color="#00A298"
                                  border-width="1.5px"
                                  font-size="10.5pt"
                                  font-family="OpenSans-Bold"
                                  font-weight="bold"
                                  padding-before="-2px"
                                  padding-after="6px"
                                  linefeed-treatment="preserve">
                            @Messages("label.now_you_have_got_your")
                            @Messages("label.national_insurance_number")
                        </fo:block>
                        <fo:block role="Div"
                                  border-after-style="solid"
                                  border-width="1.5px"
                                  border-color="#00A298"
                                  padding-after="5px">
                            <fo:block font-size="9.5pt" font-family="OpenSans" font-weight="normal" padding-before="6px"
                                      padding-after="6px">
                                <fo:block>
                                    @Messages("label.you_can_download_and_use_the_hmrc_app_or_go_online_to.text.part1")
                                </fo:block>
                                <fo:basic-link color="#1F70B8"
                                               external-destination='https://@Messages("label.you_can_download_and_use_the_hmrc_app_or_go_online_to.link")'
                                               text-decoration="underline"
                                               role="Link"
                                               fox:alt-text="HMRC app or online link">
                                    @messages("label.you_can_download_and_use_the_hmrc_app_or_go_online_to.link")
                                </fo:basic-link>
                                <fo:block>
                                    @Messages("label.you_can_download_and_use_the_hmrc_app_or_go_online_to.text.part2")
                                </fo:block>
                            </fo:block>
                            <fo:list-block role="L" provisional-distance-between-starts="0.3cm">
                                <fo:list-item font-size="9.5pt" font-family="OpenSans" font-weight="normal"
                                              line-height="11pt" padding-before="2.8px" padding-after="2.8px" role="LI">
                                    <fo:list-item-label role="Lbl" end-indent="1em">
                                        <fo:block>
                                            <fo:inline>
                                                <fo:wrapper role="artifact">
                                                    &#8226;
                                                </fo:wrapper>
                                            </fo:inline>
                                        </fo:block>
                                    </fo:list-item-label>
                                    <fo:list-item-body role="LBody" start-indent="2em">
                                        <fo:block>
                                            <fo:block>
                                                @Messages("label.create_and_access_your_personal_tax_account")
                                            </fo:block>
                                        </fo:block>
                                    </fo:list-item-body>
